Server:AddButton({Name = "Change UI Color",Callback = function()
    for _,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.NavigatorApp.Frame.Buttons:GetChildren()) do 
        if v:IsA("Frame") and tostring(string.upper(v.Name)):match("SHOP") then 
            v:FindFirstChildOfClass("ImageButton").Image = "rbxassetid://11647511954"
            v:FindFirstChildOfClass("ImageButton").ImageColor3 = Settings.GameUIColor
        elseif v:IsA("Frame") and tostring(string.upper(v.Name)):match("JOURNAL") then 
            v:FindFirstChildOfClass("ImageButton").Image = "rbxassetid://11647558189"
            v:FindFirstChildOfClass("ImageButton").ImageColor3 = Settings.GameUIColor
        elseif v:IsA("Frame") and tostring(string.upper(v.Name)):match("FRIENDS") then 
            v:FindFirstChildOfClass("ImageButton").Image = "rbxassetid://11647568655"
            v:FindFirstChildOfClass("ImageButton").ImageColor3 = Settings.GameUIColor
        elseif v:IsA("Frame") and tostring(string.upper(v.Name)):match("AVATAR") then 
            v:FindFirstChildOfClass("ImageButton").Image = "rbxassetid://11647569710"
            v:FindFirstChildOfClass("ImageButton").ImageColor3 = Settings.GameUIColor
        end
    end
    for _,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui:GetDescendants()) do 
        if v:IsA("ImageButton") then 
            v.ImageColor3 = Settings.GameUIColor
        elseif v:IsA("Frame") then 
            v.BackgroundColor3 = Color3.fromRGB(0,0,0)
        end
    end
    game:GetService("Players").LocalPlayer.PlayerGui.BucksIndicatorApp.CurrencyIndicator.Container.Amount.TextColor3 = Color3.fromRGB(0,0,0)
    game:GetService("Players").LocalPlayer.PlayerGui.BucksIndicatorApp.CurrencyIndicator.Container.Backing.ImageColor3 = Color3.fromRGB(1,255,1)
    game:GetService("Players").LocalPlayer.PlayerGui.ToolApp.Frame.OpenBackpack.ImageColor3 = Color3.fromRGB(0,0,0)
    game:GetService("Players").LocalPlayer.PlayerGui.ToolApp.Frame.OpenBackpack.BackpackIcon.ImageColor3 = Color3.fromRGB(1,255,1)
end    
})

