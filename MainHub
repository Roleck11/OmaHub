Shop:AddSection({
    Name = "// "..Apple.." Food //"
})



Shop:AddDropdown({Name="Select Food", Default=Settings.SelectedFood,Options=Foods, Callback=function(s)
    Settings.SelectedFood = tostring(s)
end})


Shop:AddButton({Name="Buy Food", Callback=function()
    local Food 
    for i,v in pairs(game.ReplicatedStorage.SharedModules.ContentPacks:GetChildren()) do 
        if v:IsA("Folder") and v:FindFirstChild("InventorySubDB") then 
            local Folder = v:FindFirstChild("InventorySubDB")
            if Folder:FindFirstChild("Food") then
               local CheckFood = require(Folder.Food)
                for i,v in pairs(CheckFood) do 
                    local FoodName = v.name  
                    local Price = tostring(v.cost)
                    if Settings.SelectedFood == tostring(string.gsub(string.gsub(FoodName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price then 
                        Food = i
                        break 
                    end
                end 
            end
        end
    end
    for i,v in pairs(FoodTable.entries) do 
        local Price = tostring(v.cost)
        local FoodName = v.name or string.gsub(i,"%p_-%a",string.upper)
        if tostring(string.gsub(string.gsub(FoodName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price == Settings.SelectedFood then 
           Food = i 
           break 
        end
    end
    if Food then 
        RS.API:FindFirstChild("ShopAPI/BuyItem"):InvokeServer("food",Food,{}) 
    end 
end})


Shop:AddToggle({Name="Auto Buy Food", Default=false, Callback=function(t)
    Settings.AutoBuyFood = t 
    UpdateFile()
    while Settings.AutoBuyFood and tonumber(require(game:service'ReplicatedStorage'.Fsys).load("ClientData").get("money")) >= 70 do  task.wait()
        for i,v in pairs(FoodTable.entries) do 
            local Price = tostring(v.cost)
            local FoodName = string.gsub(i,"%p_-%a",string.upper)
            if v.cost ~= nil and not v.not_for_sale and not v.robux and VerifyFood(i) and Settings.SelectedFood == (tostring(string.gsub(string.gsub(FoodName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price) and Settings.AutoBuyFood then
               RS.API:FindFirstChild("ShopAPI/BuyItem"):InvokeServer("food", tostring(i), {})
                task.wait(.55)
               RS.API["ToolAPI/Unequip"]:InvokeServer(game.Players.LocalPlayer.Character.GenericTool:FindFirstChild("unique").Value)
            end
        end
    end
end})

