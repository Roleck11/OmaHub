local Found 
for i,v in pairs(getgenv().EasySettings.Eazvy_Users) do 
    for x,d in pairs(v) do
        if tostring(x) == 'UWPConfig' then 
            Found = true 
        end
    end
end

if not Found then 
    local shit = { 
     ['UWPConfig'] = {
