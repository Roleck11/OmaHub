Pets:AddButton({Name="Teleport to Pet",Callback=function()
    local Last_Equipped = tostring(Client_Data.get("last_equipped_pet"))
    NewData = Data.get_data()[tostring(game:GetService("Players").LocalPlayer)]
    for i,v in pairs(NewData.inventory.pets) do 
        if tostring(v.unique) == tostring(Last_Equipped) then 
            Settings.Pet = v.unique
            Settings.PetName = GetPetName(v.kind)
            break
        end 
    end
    if Fsys("ClientData").get("pet_char_wrapper") and Fsys("ClientData").get("pet_char_wrapper")["char"] then 
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Fsys("ClientData").get("pet_char_wrapper")["char"].PrimaryPart.CFrame 
    end
end})



Pets:AddButton({Name="Bring Pet",Callback=function()
    local Last_Equipped = tostring(Client_Data.get("last_equipped_pet"))
    NewData = Data.get_data()[tostring(game:GetService("Players").LocalPlayer)]
    for i,v in pairs(NewData.inventory.pets) do 
        if tostring(v.unique) == tostring(Last_Equipped) then 
            Settings.Pet = v.unique
            Settings.PetName = GetPetName(v.kind)
            break
        end 
    end
    if  Fsys("ClientData").get("pet_char_wrapper") and Fsys("ClientData").get("pet_char_wrapper")["char"] then 
        Fsys("ClientData").get("pet_char_wrapper")["char"].PrimaryPart.CFrame = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame
    end
end})

