end}) 

Main:AddSection({
    Name = "// "..Sparkles.." Teleports //"
})

Main:AddDropdown({Name=tostring(utf8.char(0x0001F3DD)) .. " * Main Locations",Default=Settings.SelectedLocation,Options=Locations,Callback=function(s)
    Settings.SelectedLocation = tostring(s)
end})


Main:AddButton({Name="Teleport to Location", Callback=function()
    CreateTempPart()
    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
    TeleportLocation(Teleports[Settings.SelectedLocation].MainID, Teleports[Settings.SelectedLocation].SecID)
    pcall(function()
        task.spawn(function()
            game.Workspace.Interiors:WaitForChild(tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model")))
        end)
    end)
    if game.Workspace.Interiors[tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model"))].Doors:FindFirstChild("MainDoor") then 
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Interiors[tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model"))].Doors.MainDoor.WorkingParts.TouchToEnter.CFrame:ToWorldSpace(CFrame.new(0, 0, -6))
    elseif Settings.SelectedLocation:match("Main Map") then 
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-247, 19, -1519)
    elseif Teleports[Settings.SelectedLocation].ThirID then 
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Teleports[Settings.SelectedLocation].ThirID
    end
    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
    if workspace:FindFirstChild("AdoptMeAssCheeks") then 
        workspace.AdoptMeAssCheeks:Destroy() 
    end
end})




Main:AddButton({Name="Teleport to Main Map", Callback=function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
    TeleportLocation("MainMap", "Neighborhood/MainDoor", {})
    game.Workspace.Interiors:WaitForChild(tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model")))
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-247, 19, -1519)
    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
end})

Main:AddButton({Name="Teleport to Home", Callback=function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
    RS.API["HousingAPI/SubscribeToHouse"]:FireServer(game:GetService("Players").LocalPlayer)
    TeleportLocation("housing", "MainDoor", {house_owner = game:GetService("Players").LocalPlayer})
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.HouseInteriors.blueprint[tostring(workspace.HouseInteriors.blueprint:FindFirstChildWhichIsA("Model"))].Doors.MainDoor.WorkingParts.TouchToEnter.CFrame:ToWorldSpace(CFrame.new(0, 0, -9.5))
    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
end})

local function GetLocation(emote)
    for i,v in pairs(Teleports) do 
      upper_string = string.upper(i)
      upper_emote = string.upper(emote)
      if upper_string == upper_emote then 
         return i 
      end
     end
    for i,v in pairs(Teleports) do 
        lower_string = string.lower(i)
        lower_emote = string.lower(emote)
        if string.find(i, emote) or string.find(lower_string, lower_emote) then 
            return i 
        end
    end
end


Main:AddTextbox({Name = "Teleport to (Location)",Default = "",TextDisappear = true,Callback = function(s)
    Settings.SelectedLocation = GetLocation(s)
    if GetLocation(s) then 
        CreateTempPart()
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
        TeleportLocation(Teleports[Settings.SelectedLocation].MainID, Teleports[Settings.SelectedLocation].SecID)
        pcall(function()
            task.spawn(function()
                game.Workspace.Interiors:WaitForChild(tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model")))
            end)
        end)
        if game.Workspace.Interiors[tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model"))].Doors:FindFirstChild("MainDoor") then 
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Interiors[tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model"))].Doors.MainDoor.WorkingParts.TouchToEnter.CFrame:ToWorldSpace(CFrame.new(0, 0, -6))
        elseif Settings.SelectedLocation:match("Main Map") then 
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-247, 19, -1519)
        elseif Teleports[Settings.SelectedLocation].ThirID then 
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Teleports[Settings.SelectedLocation].ThirID
        end
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
        if workspace:FindFirstChild("AdoptMeAssCheeks") then 
            workspace.AdoptMeAssCheeks:Destroy() 
        end
    end 
end})


Main:AddTextbox({Name = "Teleport to (Player)",Default = "",TextDisappear = true,Callback = function(s)
    if not getPlayersByName(s) or not s or s == "" then return end 
    TeleportToPlayer:InvokeServer(getPlayersByName(s))
end})

Main:AddTextbox({Name = "Teleport to Player House",Default = "",TextDisappear = true,Callback = function(s)
    if not getPlayersByName(s) or not s or s == "" then return end 
    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
    RS.API["HousingAPI/SubscribeToHouse"]:FireServer(getPlayersByName(s))
    TeleportLocation("housing", "MainDoor", {house_owner = getPlayersByName(s)})
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.HouseInteriors.blueprint[tostring(workspace.HouseInteriors.blueprint:FindFirstChildWhichIsA("Model"))].Doors.MainDoor.WorkingParts.TouchToEnter.CFrame:ToWorldSpace(CFrame.new(0, 0, -9.5))
    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
end})



