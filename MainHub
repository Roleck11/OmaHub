            renderSettings.EditQualityLevel = "Level01"

            -- -- User Game Settings
            userGameSettings.SavedQualityLevel = Enum.SavedQualitySetting.QualityLevel1
            userGameSettings.HasEverUsedVR = false 
            userGameSettings.ChatVisible = false
            userGameSettings.AllTutorialsDisabled = false
            userGameSettings.Fullscreen = false
            userGameSettings.GraphicsQualityLevel = 0
            userGameSettings.SavedQualityLevel = 0 
            

            --Physics Settings 
            physicsSettings.AllowSleep = true 
            physicsSettings.AreAnchorsShown = false 
            physicsSettings.AreAssembliesShown = false 
            physicsSettings.AreAssemblyCentersOfMassShown = false 
            physicsSettings.AreAwakePartsHighlighted = false 
            physicsSettings.AreBodyTypesShown = false 
            physicsSettings.AreCollisionCostsShown = false 
            physicsSettings.AreConstraintForcesShownForSelectedOrHoveredInstances = false 
            physicsSettings.AreContactForcesShownForSelectedOrHoveredAssemblies = false 
            physicsSettings.AreContactIslandsShown = false 
            physicsSettings.AreContactPointsShown = false 
            physicsSettings.AreJointCoordinatesShown = false 
            physicsSettings.AreMagnitudesShownForDrawnForcesAndTorques = false 
            physicsSettings.AreMechanismsShown = false 
            physicsSettings.AreModelCoordsShown = false 
            physicsSettings.AreOwnersShown = false 
            physicsSettings.ArePartCoordsShown = false 
            physicsSettings.AreRegionsShown = false 
            physicsSettings.AreSolverIslandsShown = false 
            physicsSettings.AreTerrainReplicationRegionsShown = false 
            physicsSettings.AreTimestepsShown = false 
            physicsSettings.AreUnalignedPartsShown = false 
            physicsSettings.AreWorldCoordsShown = false 
            physicsSettings.DisableCSGv2 = true 
            physicsSettings.ForceDrawScale = 0 
            physicsSettings.IsInterpolationThrottleShown = false 
            physicsSettings.IsReceiveAgeShown = false 
            physicsSettings.IsTreeShown = false 
            physicsSettings.PhysicsEnvironmentalThrottle = Enum.EnviromentalPhysicsThrottle.Disabled
            physicsSettings.ShowDecompositionGeometry = false 
            physicsSettings.ThrottleAdjustTime = 1

            -- Network Settings
            networkSettings.PrintPhysicsErrors = false
            networkSettings.PrintJoinSizeBreakdown = false
            networkSettings.PrintStreamInstanceQuota = false
            networkSettings.RandomizeJoinInstanceOrder = false
            networkSettings.RenderStreamedRegions = false
            networkSettings.ShowActiveAnimationAsset = false
            HiddenProps(workspace, "HumanoidOnlySetCollisionsOnStateChange", Enum.HumanoidOnlySetCollisionsOnStateChange.Disabled)
            HiddenProps(workspace, "InterpolationThrottling", Enum.InterpolationThrottlingMode.Disabled)
            HiddenProps(workspace, "IKControlConstraintSupport", Enum.IKControlConstraintSupport.Disabled)
            HiddenProps(workspace, "MeshPartHeadsAndAccessories", Enum.MeshPartHeadsAndAccessories.Disabled)
            HiddenProps(workspace,"PhysicsSteppingMethod", Enum.PhysicsSteppingMethod.Adaptive)
            HiddenProps(workspace,"ReplicateInstanceDestroySetting",Enum.ReplicateInstanceDestroySetting.Disabled)
            HiddenProps(workspace,"StreamOutBehavior",Enum.StreamOutBehavior.LowMemory)
            HiddenProps(workspace,"StreamingIntegrityMode",Enum.StreamingIntegrityMode.MinimumRadiusPause)
            HiddenProps(workspace,"StreamingEnabled",true)
            HiddenProps(workspace,"StreamingMinRadius",1)
            HiddenProps(workspace,"StreamingTargetRadius",1)
            HiddenProps(workspace,"FallenPartsDestroyHeight",1)
            for i,v in pairs(game:GetService("ReplicatedStorage").ClientServices:GetDescendants()) do 
                if v:IsA("ModuleScript") and require(v) and require(v).init and v.Name ~= "DoorsClient" and v.Name ~= "ResetHandler" then 
                require(v).init = function() return end 
                end 
            end 
            for i,v in pairs(require(game:GetService("ReplicatedStorage").ClientModules.Core.WeatherClient.WeatherClient)) do 
                if type(v) == "function" then 
                v = function() return end 
                end
            end   
            for i,v in pairs(game:GetService("ReplicatedStorage").ClientModules.Game.PetEntities.PetPerformancesFolder.Emotes:GetChildren()) do 
                if v:IsA("ModuleScript") then 
                    require(v).step = function() return end 
                end
            end    
            for i,v in pairs(game:GetService("ReplicatedStorage").ClientModules.Game.PetEntities.PetReactions:GetChildren()) do 
                require(v).init_reaction = function() return end 
            end  
            for i,v in pairs(game:GetService("ReplicatedStorage").ClientModules.Game.PetEntities.PetPerformancesFolder.Transitions:GetChildren()) do 
                if v:IsA("ModuleScript") then 
                    require(v).step = function() return end 
                end
            end   
            for Data,Effect in pairs(require(game:GetService("ReplicatedStorage").ClientModules.Core.UIManager.Apps.SpecialEffectsApp)) do 
                if type(Data) == "function" then 
                    Data = function() return end 
                end
            end
            for i,v in pairs(workspace.Interiors:GetDescendants()) do 
                if v:IsA("MeshPart") or v:IsA("Part") or v:IsA("Model") or v:IsA("Folder") then 
                    v:Destroy()
                end
            end
            for i,v in pairs(workspace.HouseExteriors:GetDescendants()) do 
                if v:IsA("MeshPart") or v:IsA("Part") or v:IsA("Model") or v:IsA("Folder") then 
                    v:Destroy()
                end
            end
            if workspace.HouseInteriors.blueprint:FindFirstChildOfClass("Model") then 
                workspace.HouseInteriors.blueprint:FindFirstChildOfClass("Model"):Destroy()
            end
            for i,v in pairs(workspace.HouseInteriors.furniture:GetChildren()) do
                v:Destroy()
            end
            for i,v in pairs(workspace.StaticMap:GetChildren()) do 
                if v.Name ~= "Park" and v.Name ~= "Campsite" and v.Name ~= "Pool" and v.Name ~= "TeleportLocations" then 
                    v:Destroy()
                elseif v.Name == "Park" then 
                    for i,v in pairs(v:GetChildren()) do 
                        if v.Name ~= "BoredAilmentTarget" then 
                            v:Destroy()
                        end
                    end
                elseif v.Name == "Campsite" then 
                    for i,v in pairs(v:GetChildren()) do 
                        if v.Name ~= "CampsiteOrigin" then 
                            v:Destroy()
                        elseif v.Name == "CampsiteOrigin" then 
                            v.Transparency = 1 
                        end
                    end
                end
            end
            if enabledMax then 
                Library:MakeNotification({
                    Name = "Eazvy Hub - Success",
                    Content = "Max Performance Enabled; You may experience heavy client-lag for a few seconds when enabling this. Note: Teleports, will not work afterwards you have to disable Max Performance & rejoin the game.",
                    Image = "rbxassetid://4914902889",
                    Time = 10
                })
                loadstring(game:HttpGet("https://raw.githubusercontent.com/Eazvy/Eazvy-Hub/main/Content/FPSBooster.lua"))()
                if not game.CoreGui:FindFirstChild("ass") and not game.Players.LocalPlayer.PlayerGui.AilmentsMonitorApp:FindFirstChild("ass") then 
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/Eazvy/Eazvy-Hub/main/Content/FPSBoosterFrame.lua"))()
                end 
                enabledMax = false 
                if game.CoreGui:FindFirstChild("ass") then 
                    game.CoreGui.ass.Parent = game.Players.LocalPlayer.PlayerGui.AilmentsMonitorApp
                end 
            end 
            local a = Instance.new("Part", game.ReplicatedStorage)
            a.Name = "adoptmeisasscheeks014021"       
            Remote = game:GetService("ReplicatedStorage"):WaitForChild("API"):WaitForChild("SettingsAPI/SetSetting")
            Remote:FireServer("trade_chat", 4)
            Remote:FireServer("trade_requests", 3)
            Remote:FireServer("party_requests", 3)
            Remote:FireServer("family_requests", 3)
            Remote:FireServer("give_item_requests", 3)
            Remote:FireServer("char_icons",false)
            Remote:FireServer("friendship_bar_visibility",4)
            Remote:FireServer("friends_are_family",false)
            Remote:FireServer("background_music",false)
        end)
        -- if game:GetService("ReplicatedStorage"):FindFirstChild('Resources') then 
        --     game:GetService("ReplicatedStorage"):FindFirstChild('Resources'):Destroy()
        -- elseif game:GetService("ReplicatedStorage"):FindFirstChild('Sounds') then 
        --     game:GetService("ReplicatedStorage").Sounds:Destroy()
        -- end  
    end 
    task.spawn(function()
        pcall(function()
            while Settings.MaxPerformance do task.wait()
                game.StarterGui:SetCore('ChatActive', true)
                game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Chat, false)
                if Fsys("ClientData").get("pet_char_wrapper") and Fsys("ClientData").get("pet_char_wrapper")["char"] and Fsys("ClientData").get("pet_char_wrapper")["char"] ~= nil then 
                    for i,v in pairs(workspace.Pets:GetChildren()) do 
                        if Fsys("ClientData").get("pet_char_wrapper") and Fsys("ClientData").get("pet_char_wrapper")["char"] and Fsys("ClientData").get("pet_char_wrapper")["char"] ~= nil and v and v ~= Fsys("ClientData").get("pet_char_wrapper")["char"] then 
                            v:Destroy()
                        end
                    end
                end
                if Fsys("ClientData").get("pet_char_wrapper") and Fsys("ClientData").get("pet_char_wrapper")["char"] and Fsys("ClientData").get("pet_char_wrapper")["char"] ~= nil then 
                    for i,v in pairs(Fsys("ClientData").get("pet_char_wrapper")["char"]:GetChildren()) do 
                        if v.Name ~= "Humanoid" and v.Name ~= "HumanoidRootPart" and v.Name ~= "Head" and v.Name ~= "PetModel" and Fsys("ClientData").get("pet_char_wrapper") and Fsys("ClientData").get("pet_char_wrapper")["char"] and Fsys("ClientData").get("pet_char_wrapper")["char"] ~= nil then
                        v:Destroy()
                        end 
                        if v:FindFirstChild("PetModel") then 
                            for i,v in pairs(v:GetChildren()) do 
                                if not v.Name:match("Head") and v.Name ~= "HumanoidRootPart" and not v.Name:match("Torso") and not v.Name:match("Mount") and Fsys("ClientData").get("pet_char_wrapper") and Fsys("ClientData").get("pet_char_wrapper")["char"] and Fsys("ClientData").get("pet_char_wrapper")["char"] ~= nil then 
                                    v:Destroy()
                                end 
                            end 
                        end 
                    end 
                end 
                local HouseModel = workspace.HouseInteriors.blueprint:FindFirstChildOfClass("Model")
                if HouseModel and HouseModel.Name == game.Players.LocalPlayer.Name then 
                for i,v in pairs(HouseModel:GetChildren()) do 
                    if v.Name ~= "Floors" and v.Name ~= "Doors" and v.Name ~= "SpecialParts" then 
                        v:Destroy()
                    end
                    end
                end
                if workspace:FindFirstChild("Events") and workspace.Events:FindFirstChild("Gifts") then 
                workspace.Events.Gifts:Destroy()
                end
                for i,v in pairs(game.Lighting:GetChildren()) do 
                    if v:FindFirstChild("HumanoidRootPart") or v:IsA("Model") and v.PrimaryPart == nil then
                    v:Destroy()
                    elseif v.Name:match("Weather") then 
                        v:Destroy()
                    end 
                end 
                local HouseModel = workspace.HouseInteriors
                if HouseModel and HouseModel:FindFirstChild("furniture") then 
                for i,v in pairs(HouseModel.furniture:GetChildren()) do 
                    if not string.lower(v.Name):match(string.lower(game.Players.LocalPlayer.Name)) then 
                        v:Destroy()
                    end
                end
                end 
                local StaticMap = workspace.StaticMap
                local Park = {"Balloon", "Trampolines", "SwingSets", "PlaygroundSlide", "Roundabout", "Seesaw"}
                local Campsite = {"LodgeDoor", "LodgeFloor"}
                
                for _, v in ipairs(Park) do 
                    local parkPart = workspace.StaticMap.Park:FindFirstChild(v) or workspace.StaticMap:FindFirstChild(v)
                    if parkPart then 
                        parkPart:Destroy()
                    end
                end
                for _, v in ipairs(Campsite) do 
                    local campsitePart = workspace.StaticMap.Campsite:FindFirstChild(v)
                    if campsitePart then 
                        campsitePart:Destroy()
                    end
                end
                for _,v in pairs(workspace:GetChildren()) do 
                    if v.Name ~= game.Players.LocalPlayer.Name and v.Name ~= "Minigames" and v.Name ~= "HouseInteriors" and v.Name ~= "HouseExteriors" and v.Name ~= "Terrain" and v.Name ~= "Interiors" and v.Name ~= "SpawnLocation" and v.Name ~= "Pets" and v.ClassName ~= "Camera" and v.Name ~= "StaticMap" and v.Name ~= "Events" and v.Name ~= "AdoptMeAssCheeks" then
                        v:Destroy()
                    end 
                end      
                for i,v in pairs(game.Players:GetChildren()) do 
                    if v~= game.Players.LocalPlayer then 
                    v:Destroy()
                    end 
                end 
                for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui:GetChildren()) do 
                    if v.Name ~= "BackpackApp" and v.Name ~= "AltCurrencyIndicatorApp" and v.Name ~= "AilmentsMonitorApp" and v.Name ~= "BucksIndicatorApp" and v.Name ~= "ToolApp" and v.Name ~= "TransitionsApp" then
                    v:Destroy()
                    end 
                end       
                local UserInputService = game:GetService("UserInputService")
                local IsWindowFocused = iswindowactive and iswindowactive() or WindowFocused  
                if not IsWindowFocused then 
                    game:GetService("RunService"):Set3dRenderingEnabled(false)
                    --setfpscap(Settings.FPSCap) 
                    game.Players.LocalPlayer.PlayerGui.AilmentsMonitorApp.ass.Enabled = true 
                elseif IsWindowFocused then
                    game:GetService("RunService"):Set3dRenderingEnabled(true)
                    --setfpscap(30) 
                    game.Players.LocalPlayer.PlayerGui.AilmentsMonitorApp.ass.Enabled = false 
                end
            end 
        end)
    end)
end  
})




