Pets:AddSection({
    Name = "// "..Dog.." Pet Tab //"
})


function GetNeonParts(unique, part)
    for i,v in pairs(ContentPacks:GetChildren()) do 
        if v:IsA("Folder") and v:FindFirstChild("InventorySubDB") then 
            if v.InventorySubDB:FindFirstChild("Pets") then 
                local PetsModule = require(v.InventorySubDB.Pets)
                for Data,Pet in pairs(PetsModule) do 
                    if tostring(Data) == tostring(unique) and Pet[part] then 
                        return Pet[part]
                    end
                end
            end
        end
    end
end


Pets:AddColorpicker({Name = "Neon Color",Default = Color3.fromRGB(255, 0, 0),Callback = function(color)
    Settings.NeonColor = color 
    UpdateFile()
end    
})

Pets:AddButton({Name="Color Pet Neon", Callback=function()
    local Last_Equipped = tostring(Client_Data.get("last_equipped_pet"))
    NewData = Data.get_data()[tostring(game.Players.LocalPlayer)]
    for i,v in pairs(NewData.inventory.pets) do 
        if tostring(v.unique) == tostring(Last_Equipped) then 
            local Neon_Parts = GetNeonParts(v.kind,"neon_parts")
            if Neon_Parts then 
                for i,v in pairs(Neon_Parts) do 
                    local v5 = Fsys("ClientData").get("pet_char_wrapper")["char"].PetModel:FindFirstChild(i);
                    if v5 then
                        v5.Material = Enum.Material.Neon;
                        v5.Color = Settings.NeonColor
                        if v5:FindFirstChild("DisplayEyes") then
                            v5.DisplayEyes.Color = Settings.NeonColor
                        end
                    end
                end
            end
        end 
    end
end})






Rainbowpet = Pets:AddToggle({Name = "Rainbow Pet",Default = false,Callback = function(t)
    Settings.RainbowPet = t 
    UpdateFile()
    if Settings.RainbowPet and MainSettings.ApplySettings and MainSettings.ApplyID == tostring(game.Players.LocalPlayer.UserId) then 
        game.TextChatService.TextChannels.RBXGeneral:SendAsync("RainbowPet Enabled")
    end
    if not Settings.RainbowPet then 
        RS.API:FindFirstChild("ToolAPI/Unequip"):InvokeServer(tostring(Client_Data.get("last_equipped_pet")))
        RS.API:FindFirstChild("ToolAPI/Equip"):InvokeServer(tostring(Client_Data.get("last_equipped_pet")))
    end
    while Settings.RainbowPet do task.wait()
        if Fsys("ClientData").get("pet_char_wrapper") and Fsys("ClientData").get("pet_char_wrapper")["char"] and Fsys("ClientData").get("pet_char_wrapper")["char"]:FindFirstChild("PetModel") then 
            for _,v in pairs(Fsys("ClientData").get("pet_char_wrapper")["char"].PetModel:GetChildren()) do 
                if v:IsA("BasePart") or v:IsA("MeshPart") and v.Name ~= "DisplayEyes" and Settings.RainbowPet then 
                    v.Color = Color3.fromHSV(tick()%5/5,1,1)
                end
            end
        end
    end
 end})


Pets:AddSlider({Name = "Walkspeed",Min = 16,Max = 250,Default = 16,Color = Color3.fromRGB(160,32,240),Increment = 1,ValueName = "",Callback = function(s)
