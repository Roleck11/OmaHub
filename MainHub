PizzaFarm = Main:AddToggle({Name="Pizza Farm", Default=false, Callback=function(t)
    PizzaFarm = t 
    if PizzaFarm == true and not PizzaModule.works_here() then 
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
        TeleportLocation(Teleports['Pizza Shop'].MainID, Teleports['Pizza Shop'].SecID)
        game.Workspace.Interiors:WaitForChild(tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model")))
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Interiors[tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model"))].Doors.MainDoor.WorkingParts.TouchToEnter.CFrame:ToWorldSpace(CFrame.new(0, 0, -6))
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
        repeat wait() until workspace.Interiors:FindFirstChild("PizzaShop")
        task.wait(1)
        Status:Set("Switching Jobs...")
        RS.API["HousingAPI/ActivateInteriorFurniture"]:InvokeServer("f-1", "UseBlock", true, workspace[PS.LocalPlayer.Name])
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = FurnitureHelper.get_all_furniture_of_id("pizza_shop_conveyor_belt")[1].UseBlocks.Deliver.CFrame
    end
    while PizzaFarm == true  do task.wait()
        if workspace.HouseInteriors.furniture:FindFirstChild("nil/nil/PizzaShop/false/f-4") and not workspace.HouseInteriors.furniture:FindFirstChild("nil/nil/PizzaShop/false/f-4").PizzaShopOrderBoard.Board.RoactTree['1']:FindFirstChild("ItIsNight") or game:GetService("Players").LocalPlayer.PlayerGui.RoleplayPayApp.Container.Role.EarningsLabel.Text == "$50/$50" then 
            Status:Set("Refilling Supplies...")
            if tick() - (LastNotif or 0) > 25 then
                LastNotif = tick() 
                RefillBowls()
            end 
            Status:Set("Getting Dough...")
            GetDough()
            Status:Set("Rolling Dough...")
            RollDough()
            task.wait(.35)
            if CheckDough() then 
                Status:Set("Teleporting Pizza...")
                TopPizza()
                Status:Set("Adding Toppings...")
                MakePizzas()
                Status:Set("Getting Pizza...")
                GetPizza()
                Status:Set("Cooking Pizza...")
                CookPizzas()
                Status:Set("Waiting 20 Seconds...")
                task.wait(20)
                Status:Set("Getting Pizza...")
                GetPizzas()
                Status:Set("Delivering Pizza...")
                DeliverPizza()
            end
        else 
            if game:GetService("Players").LocalPlayer.PlayerGui.RoleplayPayApp.Container.Role.EarningsLabel.Text == "$50/$50" then 
                Status:Set("You've reached MAX PAY! | Waiting for Pizza Shop to REOPEN...")
            else 
                Status:Set("Waiting for Pizza Shop to Open...")
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = FurnitureHelper.get_all_furniture_of_id("pizza_shop_conveyor_belt")[1].UseBlocks.Deliver.CFrame + Vector3.new(0, 3, 0)
            end
        end
    end
end})

local SortPet = Main:AddDropdown({Name = "Sort (Pet-Dropdown)",Default="",Options={"Mega and Neon", "Mega Neon", "Neon", "Fly & Ride", "Fly", "Ride", "Age"},Callback = function(s)
    local Pets = {}
    NewData = Data.get_data()[tostring(game.Players.LocalPlayer)]
    for i,v in pairs(NewData.inventory.pets) do 
        Kind = string.gsub(GetPetName(v.kind) or v.kind,"%p_-%a",string.upper)
        if s == "Roleplay Name" and v.properties.rp_name ~= "" then 
            table.insert(Pets, (tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end) .. " (" .. tostring(v.properties.rp_name) .. ") " .. " - " .. v.properties.age .. " Years Old")))
        elseif v.properties.neon or v.properties.mega_neon and s == "Mega and Neon" then
            table.insert(Pets, (tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end) .. " (" .. tostring(v.properties.rp_name) .. ") " .. " - " .. v.properties.age .. " Years Old")))
        elseif v.properties.mega_neon and s == "Mega Neon" then 
            table.insert(Pets, (tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end) .. " (" .. tostring(v.properties.rp_name) .. ") " .. " - " .. v.properties.age .. " Years Old")))
        elseif v.properties.neon and s == "Neon" then 
            table.insert(Pets, (tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end) .. " (" .. tostring(v.properties.rp_name) .. ") " .. " - " .. v.properties.age .. " Years Old")))
        elseif v.properties.flyable and s == "Fly" then 
            table.insert(Pets, (tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end) .. " (" .. tostring(v.properties.rp_name) .. ") " .. " - " .. v.properties.age .. " Years Old")))
        elseif v.properties.rideable and s == "Ride" then 
            table.insert(Pets, (tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end) .. " (" .. tostring(v.properties.rp_name) .. ") " .. " - " .. v.properties.age .. " Years Old")))
        elseif v.properties.flyable and v.properties.rideable and s == "Fly & Ride Only" then 
            table.insert(Pets, (tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end) .. " (" .. tostring(v.properties.rp_name) .. ") " .. " - " .. v.properties.age .. " Years Old")))
        end
    end
    CurrentPets:Refresh(Pets, true)
end    
})




