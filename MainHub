    Name = "// "..Potion.." Potions //"
})


Shop:AddDropdown({Name="Select Potion", Default=Settings.SelectedPotion,Options=Potions, Callback=function(s)
    Settings.SelectedPotion = tostring(s)
end})

Shop:AddButton({Name="Buy Potion", Callback=function()
    local Potion 
    for i,v in pairs(game.ReplicatedStorage.SharedModules.ContentPacks:GetChildren()) do 
        if v:IsA("Folder") and v:FindFirstChild("InventorySubDB") then 
            local Folder = v:FindFirstChild("InventorySubDB")
            if Folder:FindFirstChild("Food") then
               local CheckFood = require(Folder.Food)
                for i,v in pairs(CheckFood) do 
                    local FoodName = v.name  
                    local Price = tostring(v.cost)
                    if Settings.SelectedPotion == tostring(string.gsub(string.gsub(FoodName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price then 
                        Potion = i
                        break 
                    end
                end 
            end
        end
    end
    
    for i,v in pairs(FoodTable.entries) do 
        local Price = tostring(v.cost)
        local FoodName = CheckFood.name or string.gsub(i,"%p_-%a",string.upper)
        if tostring(string.gsub(string.gsub(FoodName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price.SelectedPotion then 
           Potion = i 
           break 
        end
    end
    if Potion then 
        RS.API:FindFirstChild("ShopAPI/BuyItem"):InvokeServer("food", tostring(Potion), {})
    end 
end})

Shop:AddSection({
    Name = "// "..Present .." Gifts //"
})

Shop:AddDropdown({Name="Select Gift",Default="",Options=Gifts, Callback=function(s)
    Settings.SelectedGift = tostring(s) 
end})


Shop:AddButton({Name="Buy Gift", Callback=function()
    local Gift
    for i,v in pairs(CurrentGifts.entries) do
        local Price = tostring(v.cost) 
        local GiftName = v.name
        if v.cost ~= nil and not v.not_for_sale and Settings.SelectedGift == tostring(string.gsub(string.gsub(GiftName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price then 
           Gift = i 
           break 
        end
    end 
    for i,v in pairs(game.ReplicatedStorage.SharedModules.ContentPacks:GetChildren()) do 
        if v:IsA("Folder") and v:FindFirstChild("InventorySubDB") then 
            local Folder = v:FindFirstChild("InventorySubDB")
            if Folder:FindFirstChild("Gifts") then 
                for i,v in pairs(require(Folder.Gifts)) do 
                    if v.cost ~= nil and not v.not_for_sale then 
                        local Price = tostring(v.cost) 
                        local GiftName = v.name
                        if GiftName == Settings.SelectedGift then 
                           Gift = i 
                           break 
                        end
                    end
                end
            end
        end
    end
   if Gift then 
      RS.API:FindFirstChild("ShopAPI/BuyItem"):InvokeServer("gifts", Gift, {})
   end 
end})

Shop:AddToggle({Name="Auto Buy Gifts", Default=false, Callback=function(t)
    Settings.AutoBuyGifts = t 
    UpdateFile()
    while Settings.AutoBuyGifts and tonumber(require(game:service'ReplicatedStorage'.Fsys).load("ClientData").get("money")) and tonumber(require(game:service'ReplicatedStorage'.Fsys).load("ClientData").get("money")) >= 70 do 
        local Gift
        for i,v in pairs(CurrentGifts.entries) do
            local Price = tostring(v.cost) 
            local GiftName = string.gsub(i,"%p_-%a",string.upper)
            if v.cost ~= nil and not v.not_for_sale then 
                if Settings.SelectedGift == (tostring(string.gsub(string.gsub(GiftName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price) then 
                Gift = i
                break 
                end
            end
        end    
        for i,v in pairs(game.ReplicatedStorage.SharedModules.ContentPacks:GetChildren()) do 
            if v:IsA("Folder") and v:FindFirstChild("InventorySubDB") then 
                local Folder = v:FindFirstChild("InventorySubDB")
                if Folder:FindFirstChild("Gifts") then 
                    for i,v in pairs(require(Folder.Gifts)) do 
                        if v.cost ~= nil and not v.not_for_sale then 
                            local Price = tostring(v.cost) 
                            local GiftName = v.name
                            if GiftName == Settings.SelectedGift then 
                               Gift = i 
                               break 
                            end
                        end
                    end
                end
            end
        end
        RS.API:FindFirstChild("ShopAPI/BuyItem"):InvokeServer("gifts", Gift, {})
        task.wait()
    end
end})

Shop:AddToggle({Name="Auto Open Gifts", Default=false , Callback=function(t)
    Settings.AutoOpenGifts = t 
    UpdateFile()
    while Settings.AutoOpenGifts == true do wait()
        NewData = Data.get_data()[tostring(PS.LocalPlayer)]
        for i,v in pairs(NewData.inventory.gifts) do 
            if string.lower(tostring(v.kind)):match("gift") then 
                local OpenGift = RS.API:FindFirstChild("ShopAPI/OpenGift"):InvokeServer(tostring(i))
                Kind = string.gsub(GetPetName(v.kind) or v.kind,"%p_-%a",string.upper)
                if tick() - (LastNotif or 0) > .35 and Settings.AutoOpenGifts then
					LastNotif = tick() 
					Library:MakeNotification({
						Name = "Eazvy Hub - Success",
						Content = "Claimed " .. OpenGift .. " from: " .. tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end)),
						Image = "rbxassetid://4914902889",
						Time = 1.4
					})
				end
            elseif string.lower(tostring(v.kind)):match("box") or string.lower(tostring(v.kind)):match("chest") then 
                game:GetService("ReplicatedStorage").API["LootBoxAPI/ExchangeItemForReward"]:InvokeServer(tostring(v.kind), tostring(i))
            end
        end
    end
end})





Shop:AddSection({
    Name = "// " .. Dollar .. " Robux Pets //"
})

Shop:AddDropdown({Name = "Select Pet",Default=Settings.SelectedRobuxPet,Options=RobuxPets,Callback = function(s)
    Settings.SelectedRobuxPet = s 
end    
})

Shop:AddButton({Name="Buy Pet", Callback=function()
    for i,v in pairs(RS.SharedModules.ContentPacks:GetChildren()) do 
        if v:IsA("Folder") and v:FindFirstChild("InventorySubDB") then 
            local Folder = v:FindFirstChild("InventorySubDB")
            if Folder:FindFirstChild("Pets") then 
                for i,v in pairs(require(Folder.Pets)) do 
                    local Check_Pet = require(Folder.PetsOrigin)[i]
                    if Check_Pet.robux and require(game:GetService("ReplicatedStorage").ClientDB.DeveloperProductDB)[i] and require(game:GetService("ReplicatedStorage").ClientDB.DeveloperProductDB)[i].name then 
                        local Name = require(game:GetService("ReplicatedStorage").ClientDB.DeveloperProductDB)[i].name
                        local Cost = require(game:GetService("ReplicatedStorage").ClientDB.DeveloperProductDB)[i].cost
                        if Settings.SelectedRobuxPet == (Name .. " | Price: " .. tostring(Cost) .. " Robux") then 
                            game:GetService("ReplicatedStorage").API["ProductsAPI/PromptProductPurchase"]:FireServer(i)
                        end 
                    end 
                end
            end
        end
    end    
end})

