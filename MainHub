Flying = LP:AddToggle({Name = "Fly",Default = false,Callback = function(t)
    Settings.Fly = t 
    UpdateFile()
    if Settings.Fly and MainSettings.ApplySettings and MainSettings.ApplyID == tostring(game.Players.LocalPlayer.UserId) then 
        game.TextChatService.TextChannels.RBXGeneral:SendAsync("Fly Enabled")
    end
    if Settings.Fly then 
        for Every,Connection in next, getconnections(game:GetService("Players").LocalPlayer.Character:FindFirstChild("Head").ChildAdded) do 
            Connection:Disable()
        end
        StartFly();
    else 
        EndFly();
    end
end})

NoShit = LP:AddToggle({Name = "Noclip",Default = false,Callback = function(t)
    Noclip = t 
    function noClip()
        if Noclip and PS.LocalPlayer.Character ~= nil then 
            for _, child in pairs(PS.LocalPlayer.Character:GetDescendants()) do 
                if child:IsA('BasePart') and child.CanCollide then 
                    child.CanCollide = false 
                end
            end
        end
    end 
    Noclipping = game:GetService("RunService").Stepped:Connect(noClip) 
end})


Rainbo = LP:AddToggle({Name = "Rainbow Character",Default = false,Callback = function(t)
   Settings.RainbowChar = t
   UpdateFile()
   if Settings.RainbowChar and MainSettings.ApplySettings and MainSettings.ApplyID == tostring(game.Players.LocalPlayer.UserId) then 
    game.TextChatService.TextChannels.RBXGeneral:SendAsync("RainbowChar Enabled")
   end
   while Settings.RainbowChar do wait()
        for i,v in pairs(Skin_Tones) do 
            if Settings.RainbowChar then 
                RS.API["AvatarAPI/PutOn"]:InvokeServer("player", "skin_tone", v)
            end
        end    
    end
end})

