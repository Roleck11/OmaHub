Trolling:AddSection({
    Name = "// "..Warning.." Abusive //"
})

Trolling:AddButton({Name="Kill All Babies", Default=false, Callback=function(t)
    local OriginalPosition = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position 
    if PS.LocalPlayer.Team.Name == "Babies" then 
        RS.API:FindFirstChild("TeamAPI/ChooseTeam"):InvokeServer("Parents",true)
    end
    for _,v in pairs(game:GetService("Teams").Babies:GetPlayers()) do 
        if v.Name ~= game:GetService("Players").LocalPlayer.Name and v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") ~= nil and v.Name ~= game:GetService("Players").LocalPlayer then
            TeleportToPlayer:InvokeServer(v)
            while true do wait() if game:GetService("Players").LocalPlayer:DistanceFromCharacter(v.Character:FindFirstChild("HumanoidRootPart").Position) <= 40 then break end end
            wait(.35)
            for i=1,15 do 
                RS.API["AdoptAPI/HoldBaby"]:FireServer(v.Character)
            end
            wait(.55)
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-191, -430, -1393)
            wait(.5)
            RS.API["AdoptAPI/EjectBaby"]:FireServer(v.Character) 
            wait(.15)
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(OriginalPosition)
        end
    end
    RS.API["TeamAPI/Spawn"]:InvokeServer()
end})


TradeAllAnnoy = Trolling:AddToggle({Name="Trade All (Annoy)", Default=false, Callback=function(t)
    Settings.TradeAll = t 
    UpdateFile()
    if Settings.TradeAll and MainSettings.ApplySettings and MainSettings.ApplyID == tostring(game.Players.LocalPlayer.UserId) then 
        game.TextChatService.TextChannels.RBXGeneral:SendAsync("TradeAll Enabled")
    end
    if not Settings.TradeAll then fori=1,15 do RS.API:FindFirstChild("TradeAPI/DeclineTrade"):FireServer() end end 
    while Settings.TradeAll do wait()
        for _,v in pairs(game:GetService("Players"):GetPlayers()) do 
            if v~= game:GetService("Players").LocalPlayer and Settings.TradeAll then 
                RS.API:FindFirstChild("TradeAPI/SendTradeRequest"):FireServer(v)
                RS.API:FindFirstChild("TradeAPI/DeclineTrade"):FireServer()
                RS.API:FindFirstChild("TradeAPI/AcceptOrDeclineTradeRequest"):InvokeServer(v, false)
                wait(.15)
            end
        end
    end
end})


CopyMessages = Trolling:AddToggle({Name = "Copy Messages (Trolling)",Default = false,Callback = function(t)
    Settings.ChatLogger = t
    if Settings.ChatLogger and MainSettings.ApplySettings and MainSettings.ApplyID == tostring(game.Players.LocalPlayer.UserId) then 
        game.TextChatService.TextChannels.RBXGeneral:SendAsync("ChatLogger Enabled")
    end
    UpdateFile()
end    
})

LoopTeleport = Trolling:AddToggle({Name="Loop Teleport Babies", Default=false, Callback=function(t)
    Settings.Pickup = t 
    UpdateFile()
    if Settings.Pickup then 
        Dialog.apps.HintApp:hint({
            text = "This will only work if the babies are 40 studs away from you.",
            length = 4, 
            yields = false
        });    
        while Settings.Pickup == true do wait()
            for _,v in pairs(game:GetService("Teams").Babies:GetPlayers()) do 
                if v.Character:FindFirstChild("HumanoidRootPart") and PS.LocalPlayer:DistanceFromCharacter(v.Character.HumanoidRootPart.Position) <= 40 then 
                    RS.API["AdoptAPI/HoldBaby"]:FireServer(v.Character)
                    RS.API["AdoptAPI/EjectBaby"]:FireServer(v.Character) 
                end
            end        
        end
    end
end})


LoopHold = Trolling:AddToggle({Name="Loop Hold Babies", Default=false, Callback=function(t)
    Pickup = t 
    if Pickup then 
        Dialog.apps.HintApp:hint({
            text = "This will only work if the babies are 40 studs away from you.",
            length = 4, 
            yields = false
        });    
        while Pickup == true do wait()
            for _,v in pairs(game:GetService("Teams").Babies:GetPlayers()) do 
                if v and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and PS.LocalPlayer:DistanceFromCharacter(v.Character.HumanoidRootPart.Position) <= 40 then 
                    RS.API["AdoptAPI/HoldBaby"]:FireServer(v.Character)
                end
            end        
        end
    end
end})

local InfectAll = Trolling:AddToggle({Name="Infect All", Default=false, Callback=function(t)
    AutoSick = t 
    if AutoSick == true then 
        Dialog.apps.HintApp:hint({
            text = "Make sure your pet is sick first before enabling this. And Pets / Babies are at least 15 studs away from you.",
            length = 4, 
            yields = false
        }); 
    end 
    while AutoSick == true do wait()
        for _,v in pairs(workspace.Pets:GetChildren()) do 
            if v:IsA('Model') and v:FindFirstChild("HumanoidRootPart") and PS.LocalPlayer:DistanceFromCharacter(v:FindFirstChild("HumanoidRootPart").Position) <= 15 then 
                RS.API:FindFirstChild("MonitorAPI/InfectPlayerWithSicknessAilment"):FireServer(Fsys("ClientData").get("pet_char_wrapper")["char"], v)
            end
        end
    end
end})


House:AddSection({
    Name = "// House Tab //"
})

RainbowHouse = House:AddToggle({Name="Rainbow House",Default = Settings.RainbowHouse,Callback = function(t)
    Settings.RainbowHouse = t 
    UpdateFile()
    if Settings.RainbowHouse and MainSettings.ApplySettings and MainSettings.ApplyID == tostring(game.Players.LocalPlayer.UserId) then 
        game.TextChatService.TextChannels.RBXGeneral:SendAsync("RainbowHouse Enabled")
    end
    local House = 1 
    if Settings.RainbowHouse then  
        RS.API["HousingAPI/UnsubscribeFromHouse"]:InvokeServer(game:GetService("Players").LocalPlayer)
        RS.API["HousingAPI/SubscribeToHouse"]:FireServer(game:GetService("Players").LocalPlayer)
        task.wait(.15)
        House = require(game:service'ReplicatedStorage'.Fsys).load("ClientData").get("house_interior")
    end
    while Settings.RainbowHouse do wait()
        RS.API["HousingAPI/ColorHouse"]:FireServer(House.house_id, Color3.fromHSV(tick()%5/5,1,1))
    end
end})


RainbowFurniture = House:AddToggle({Name="Rainbow Furniture",Default = false,Callback = function(t)
    Settings.RainbowFurniture = t 
    UpdateFile()
    if Settings.RainbowFurniture and MainSettings.ApplySettings and MainSettings.ApplyID == tostring(game.Players.LocalPlayer.UserId) then 
        game.TextChatService.TextChannels.RBXGeneral:SendAsync("RainbowFurniture Enabled")
    end
    while Settings.RainbowFurniture == true do wait()
        for i,v in pairs(workspace.HouseInteriors.furniture:GetChildren()) do 
            if v:IsA("Folder") and v:FindFirstChildOfClass("Model") and Settings.RainbowFurniture == true then
                RS.API["HousingAPI/ColorFurniture"]:FireServer(v:FindFirstChildOfClass("Model"):GetAttribute("furniture_unique"), Color3.fromHSV(tick()%5/5,1,1))
            end
        end
    end
end})


House:AddButton({Name="Lock/Unlock House", Callback=function()
    require(RS.ClientModules.Game.HouseClient).lock_door()
end})

House:AddButton({Name="Throw Party", Callback=function()
    game:GetService("ReplicatedStorage").API["HousingAPI/ThrowParty"]:InvokeServer({['party_name']="Party", ['party_description']="JOIN MY PARTY!"})
end})


House:AddButton({Name="Sell all Furniture House", Callback=function()
    for _,v in pairs(Fsys("ClientData").get_server(game:GetService("Players").LocalPlayer, "char_wrapper").location) do 
        if _ == "destination_id" and v == "housing" then 
            _G.HouseFurniture = {}
            for _,v in pairs(workspace.HouseInteriors.furniture:GetChildren()) do 
                if v:IsA("Folder") and v:FindFirstChildOfClass("Model") then
                    table.insert(_G.HouseFurniture, v:FindFirstChildOfClass("Model"))
                end
            end
        elseif _ == "destination_id" and not v == "housing" then 
            RS.API["HousingAPI/SubscribeToHouse"]:FireServer(game:GetService("Players").LocalPlayer)
            task.wait(1)
            _G.HouseFurniture = {}
            for _,v in pairs(workspace.HouseInteriors.furniture:GetChildren()) do 
                if v:IsA("Folder") and v:FindFirstChildOfClass("Model") then
                    table.insert(_G.HouseFurniture, v:FindFirstChildOfClass("Model"))
                end
            end
        end
    end
    for i,v in pairs(_G.HouseFurniture) do 
        game:GetService("ReplicatedStorage").API["HousingAPI/SellFurniture"]:FireServer(v:GetAttribute("furniture_unique"))
    end    
end})



