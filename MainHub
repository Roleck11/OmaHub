House:AddSection({
    Name = "// Trade Settings //"
})

House:AddDropdown({Name = "Trade Filter",Default = "Pets",Options={"Pets","Vehicles","Accessories","Strollers","Gifts","Food","Toys"},Callback = function(s)
    local ExportedInventory = {}
    local New_Data = Data.get_data()[tostring(game.Players.LocalPlayer)]
    local MegaNeons = 0 
    local Neons = 0 
    local Flyables = 0 
    local Rideables = 0 
    local NoOverides = {}
    DataPets = {}
    if s == "Pets" then 
        NewData = Data.get_data()[tostring(game.Players.LocalPlayer)]
        for i,v in pairs(NewData.inventory.pets) do 
            if DataPets[v.kind] then 
                DataPets[v.kind] = DataPets[v.kind] + 1 
            else 
                DataPets[v.kind] = 1 
            end
        end
        if MegaNeons ~= 0 and Neons ~= 0 and Flyables ~= 0 and Rideables ~= 0 then 
            table.insert(ExportedInventory, "\nMega Neons " .. tostring(MegaNeons) .. "\nNeons: " .. tostring(Neons) .. "\nFlyable Pets: " .. tostring(Flyables) .. "\nRideable Pets: " .. tostring(Rideables) .. "\n")
        end
        for i,v in pairs(New_Data.inventory.pets) do 
            if v.properties.mega_neon then 
            MegaNeons = MegaNeons + 1 
            end
            if v.properties.neon then 
                Neons = Neons + 1 
            end
            if v.properties.flyable then 
                Flyables = Flyables + 1 
            end
            if v.properties.rideable then 
                Rideables = Rideables + 1 
            end
            if not table.find(NoOverides, v.kind) then 
                Kind = string.gsub(GetPetName(v.kind) or v.kind,"%p_-%a",string.upper)
                table.insert(ExportedInventory, "["..tostring(DataPets[v.kind]).."] "..tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end)))
            end
        end
        SelectItem:Refresh(ExportedInventory,true)
    end
    if s == "Toys" then 
        DataPets = {}
        NewData = Data.get_data()[tostring(game.Players.LocalPlayer)]
        for i,v in pairs(NewData.inventory.toys) do 
            if DataPets[v.kind] then 
                DataPets[v.kind] = DataPets[v.kind] + 1 
            else 
                DataPets[v.kind] = 1 
            end
        end
        for i,v in pairs(New_Data.inventory.toys) do 
            local Transport = require(RS.ClientDB.Inventory.InventoryToysSubDB).entries[v.kind]
            if not Transport.price then 
            Price = "Free"
            Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            elseif Transport.cost then 
                Price = tostring(Transport.cost)
                Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            else 
                Price = tostring(Transport.price)
                Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            end
            if not table.find(NoOverides, v.kind) then 
                Kind = string.gsub(GetPetName(v.kind) or v.kind,"%p_-%a",string.upper)
                table.insert(ExportedInventory, "["..tostring(DataPets[v.kind]).."] "..tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end)))
                table.insert(NoOverides, v.kind)
            end
        end
        SelectItem:Refresh(ExportedInventory,true)
    end
    if s == "Food" then 
        DataPets = {}
        NewData = Data.get_data()[tostring(game.Players.LocalPlayer)]
        for i,v in pairs(NewData.inventory.food) do 
            if DataPets[v.kind] then 
                DataPets[v.kind] = DataPets[v.kind] + 1 
            else 
                DataPets[v.kind] = 1 
            end
        end
        for i,v in pairs(New_Data.inventory.food) do 
            local Transport = require(RS.ClientDB.Inventory.InventoryFoodSubDB).entries[v.kind]
            if not Transport.price then 
            Price = "Free"
            Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            elseif Transport.cost then 
                Price = tostring(Transport.cost)
                Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            else 
                Price = tostring(Transport.price)
                Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            end
            if not table.find(NoOverides, v.kind) then 
                Kind = string.gsub(GetPetName(v.kind) or v.kind,"%p_-%a",string.upper)
                table.insert(ExportedInventory, "["..tostring(DataPets[v.kind]).."] "..tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end)))
                table.insert(NoOverides, v.kind)
            end
        end
        SelectItem:Refresh(ExportedInventory,true)
    end
    if s == "Gifts" then 
        DataPets = {}
        NewData = Data.get_data()[tostring(game.Players.LocalPlayer)]
        for i,v in pairs(NewData.inventory.gifts) do 
            if DataPets[v.kind] then 
                DataPets[v.kind] = DataPets[v.kind] + 1 
            else 
                DataPets[v.kind] = 1 
            end
        end
        for i,v in pairs(New_Data.inventory.gifts) do 
            local Transport = require(RS.ClientDB.Inventory.InventoryGiftsSubDB).entries[v.kind]
            if not Transport.price then 
            Price = "Free"
            Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            elseif Transport.cost then 
                Price = tostring(Transport.cost)
                Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            else 
                Price = tostring(Transport.price)
                Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            end
            if not table.find(NoOverides, v.kind) then 
                Kind = string.gsub(GetPetName(v.kind) or v.kind,"%p_-%a",string.upper)
                table.insert(ExportedInventory, "\n" .. tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " ={ \nPrice: " .. Price .. "\nRarity: " .. Rarity ..  "\n}")
                table.insert(NoOverides, v.kind)
            end
        end
        SelectItem:Refresh(ExportedInventory,true)
    end
    if s == "Accessories" then 
        DataPets = {}
        NewData = Data.get_data()[tostring(game.Players.LocalPlayer)]
        for i,v in pairs(NewData.inventory.pet_accessories) do 
            if DataPets[v.kind] then 
                DataPets[v.kind] = DataPets[v.kind] + 1 
            else 
                DataPets[v.kind] = 1 
            end
        end
        for i,v in pairs(New_Data.inventory.pet_accessories) do 
            local Transport = require(RS.ClientDB.Inventory.InventoryPetAccessoriesSubDB).entries[v.kind]
            if not Transport.price then 
            Price = "Free"
            Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            elseif Transport.cost then 
                Price = tostring(Transport.cost)
                Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            else 
                Price = tostring(Transport.price)
                Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            end
            if not table.find(NoOverides, v.kind) then 
                Kind = string.gsub(GetPetName(v.kind) or v.kind,"%p_-%a",string.upper)
                table.insert(ExportedInventory, "["..tostring(DataPets[v.kind]).."] "..tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end)))
                table.insert(NoOverides, v.kind)
            end
        end
        SelectItem:Refresh(ExportedInventory,true)
    end
    if s == "Strollers" then 
        DataPets = {}
        NewData = Data.get_data()[tostring(game.Players.LocalPlayer)]
        for i,v in pairs(NewData.inventory.strollers) do 
            if DataPets[v.kind] then 
                DataPets[v.kind] = DataPets[v.kind] + 1 
            else 
                DataPets[v.kind] = 1 
            end
        end
        for i,v in pairs(New_Data.inventory.strollers) do 
            local Transport = require(RS.ClientDB.Inventory.InventoryStrollersSubDB).entries[v.kind]
            if not Transport.price then 
            Price = "Free"
            Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            elseif Transport.cost then 
                Price = tostring(Transport.cost)
                Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            else 
                Price = tostring(Transport.price)
                Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            end
            if not table.find(NoOverides, v.kind) then 
                Kind = string.gsub(GetPetName(v.kind) or v.kind,"%p_-%a",string.upper)
                table.insert(ExportedInventory, "["..tostring(DataPets[v.kind]).."] "..tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end)))
                table.insert(NoOverides, v.kind)
            end
        end
        SelectItem:Refresh(ExportedInventory,true)
    end
    if s == "Vehicles" then 
        DataPets = {}
        NewData = Data.get_data()[tostring(game.Players.LocalPlayer)]
        for i,v in pairs(NewData.inventory.transport) do 
            if DataPets[v.kind] then 
                DataPets[v.kind] = DataPets[v.kind] + 1 
            else 
                DataPets[v.kind] = 1 
            end
        end
        for i,v in pairs(New_Data.inventory.transport) do 
            local Transport = require(RS.ClientDB.Inventory.InventoryTransportSubDB).entries[v.kind]
            if not Transport.price then 
            Price = "Free"
            Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            elseif Transport.cost then 
                Price = tostring(Transport.cost)
                Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            else 
                Price = tostring(Transport.price)
                Rarity = tostring(string.gsub(string.gsub(Transport.rarity, "_", " "), "^%l", function(c) return string.upper(c) end))
            end
            if not table.find(NoOverides, v.kind) then 
                Kind = string.gsub(GetPetName(v.kind) or v.kind,"%p_-%a",string.upper)
                table.insert(ExportedInventory, "["..tostring(DataPets[v.kind]).."] "..tostring(string.gsub(string.gsub(Kind, "_", " "), "^%l", function(c) return string.upper(c) end)))
                table.insert(NoOverides, v.kind)
            end
        end
        SelectItem:Refresh(ExportedInventory,true)
    end
end})

AutoAcceptAll = House:AddToggle({Name="Auto Accept All",Default = false,Callback = function(t)
    AutoAccept = t 
    if Settings.Pet_Name == "None" then return end 
    if Settings.PlayerName == "None" then return end 
    while AutoAccept == true do wait() 
        RS.API["TradeAPI/AcceptOrDeclineTradeRequest"]:InvokeServer(Settings.PlayerName, true)
        RS.API["TradeAPI/AcceptNegotiation"]:FireServer()
        RS.API["TradeAPI/ConfirmTrade"]:FireServer()
    end
end})





