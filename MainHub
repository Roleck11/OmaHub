Shop:AddSection({
    Name = "// "..Cow.. " Pets //"
})

Shop:AddDropdown({Name = "Select Pet",Default=Settings.PetSelected,Options=BuyPets,Callback = function(s)
    Settings.PetSelected = tostring(s)
end})

Shop:AddButton({Name="Buy Pet", Callback=function()
    for i,v in pairs(RS.SharedModules.ContentPacks:GetChildren()) do 
        if v:IsA("Folder") and v:FindFirstChild("InventorySubDB") then 
            local Folder = v:FindFirstChild("InventorySubDB")
            if Folder:FindFirstChild("Pets") then 
                for i,v in pairs(require(Folder.Pets)) do 
                    local Check_Pet = require(Folder.PetsOrigin)[i]
                    if (v.cost ~= nil or Check_Pet.origin=="Dolls") and not v.not_for_sale and Check_Pet.origin ~= "Task Board Update" and not Check_Pet.robux then 
                        PetAccessName = string.gsub(GetPetName(i) or i,"%p_-%a",string.upper)
                        Price = tostring(v.cost)
                        if v.cost == nil then 
                            Price = "Free"
                        end
                        if PetAccessName:match("hallow") and tostring(Settings.PetSelected) == (tostring(string.gsub(string.gsub(PetAccessName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price .. " Candy") then 
                            RS.API["ShopAPI/BuyItem"]:InvokeServer("pets", tostring(i), {})
                            break
                        elseif tostring(Settings.PetSelected) == (tostring(string.gsub(string.gsub(PetAccessName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price .. " Bucks") then 
                            RS.API["ShopAPI/BuyItem"]:InvokeServer("pets", tostring(i), {})
                            break
                        elseif PetAccessName:match("winter") and tostring(Settings.PetSelected) == (tostring(string.gsub(string.gsub(PetAccessName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price .. " Gingerbread") then 
                            RS.API["ShopAPI/BuyItem"]:InvokeServer("pets", tostring(i), {})
                            break
                        elseif PetAccessName:match("summer") and tostring(Settings.PetSelected) == (tostring(string.gsub(string.gsub(PetAccessName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price .. " Beach Balls") then 
                            RS.API["ShopAPI/BuyItem"]:InvokeServer("pets", tostring(i), {})
                            break
                        elseif tostring(Settings.PetSelected) == tostring(string.gsub(string.gsub(PetAccessName, "_", " "), "^%l", function(c) return string.upper(c) end)) then 
                            RS.API["ShopAPI/BuyItem"]:InvokeServer("pets", tostring(i), {})
                            break
                        end
                    end 
                end
            end
        end
    end
    for i,v in pairs(require(game:service'ReplicatedStorage'.Fsys).load("InventoryPetsOriginSubDB").entries) do 
        for x,d in pairs(v) do 
            if x ~= "origin_items" then 
               PetAccessName = string.gsub(i,"%p_-%a",string.upper)
               local Price = GetPrice2(i) or " Free"
               if tostring(Settings.PetSelected) == tostring(string.gsub(string.gsub(PetAccessName, "_", " "), "^%l", function(c) return string.upper(c) end) .. Price) then 
                 RS.API["ShopAPI/BuyItem"]:InvokeServer("pets", tostring(i), {})
                 break
                end 
            end 
        end            
    end 
end})



Shop:AddToggle({Name="Auto Buy Pet", Default=false, Callback=function(t)
    AutoBuyPet = t 
    while AutoBuyPet == true and tonumber(require(game:service'ReplicatedStorage'.Fsys).load("ClientData").get("money")) >= 70 do task.wait()
        for i,v in pairs(RS.SharedModules.ContentPacks:GetChildren()) do 
            if v:IsA("Folder") and v:FindFirstChild("InventorySubDB") then 
                local Folder = v:FindFirstChild("InventorySubDB")
                if Folder:FindFirstChild("Pets") then 
                    for i,v in pairs(require(Folder.Pets)) do 
                        local Check_Pet = require(Folder.PetsOrigin)[i]
                        if (v.cost ~= nil or Check_Pet.origin=="Dolls") and not v.not_for_sale and Check_Pet.obtainable and Check_Pet.origin ~= "Task Board Update" and not Check_Pet.robux then 
                            PetAccessName = string.gsub(i,"%p_-%a",string.upper)
                            Price = tostring(v.cost)
                            if v.cost == nil then 
                                Price = "Free"
                            end
                            if PetAccessName:match("hallow") and tostring(Settings.PetSelected) == (tostring(string.gsub(string.gsub(PetAccessName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price .. " Candy") then 
                                RS.API["ShopAPI/BuyItem"]:InvokeServer("pets", tostring(i), {})
                            elseif tostring(Settings.PetSelected) == (tostring(string.gsub(string.gsub(PetAccessName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price .. " Bucks") then 
                                RS.API["ShopAPI/BuyItem"]:InvokeServer("pets", tostring(i), {})
                            end
                        end 
                    end
                end
            end
        end
    end
end})




Shop:AddSection({
    Name = "// "..EggsD .." Eggs //"
})


Shop:AddDropdown({Name = "Select Egg",Default =Settings.SelectedEgg,Options=Eggs,Callback = function(s)
    Settings.SelectedEgg = s
end    
})

Shop:AddButton({Name="Buy Egg", Callback=function()
    if tostring(Settings.SelectedEgg) == "Retired Egg | Price: 600 Bucks" then 
        RS.API["ShopAPI/BuyItem"]:InvokeServer("pets", "retired_egg", {})
        return 
    end
    for i,v in pairs(RS.SharedModules.ContentPacks:GetChildren()) do 
        if v:IsA("Folder") and v:FindFirstChild("InventorySubDB") then 
            local Folder = v:FindFirstChild("InventorySubDB")
            if Folder:FindFirstChild("Pets") then 
                for i,v in pairs(require(Folder.Pets)) do 
                    local Check_Pet = require(Folder.PetsOrigin)[i]
                    if v.cost ~= nil and not v.not_for_sale and Check_Pet.origin ~= "Task Board Update" and Check_Pet.origin ~= "Rain" and Check_Pet.origin ~= "Desert Update" and Check_Pet.origin ~= "Fall Update" and not Check_Pet.robux and not v.vip_exclusive and VerifyObtain(i) then 
                        PetAccessName = string.gsub(GetPetName(i) or i,"%p_-%a",string.upper)
                        Price = tostring(v.cost)
                        if v.cost == nil then 
                            Price = "Free"
                        end
                        if Settings.SelectedEgg == (tostring(string.gsub(string.gsub(PetAccessName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price .. " Bucks") then 
                            RS.API["ShopAPI/BuyItem"]:InvokeServer("pets", i, {})
                            return 
                        end 
                    end 
                end 
            end 
        end 
    end 
    for i,v in pairs(require(game:service'ReplicatedStorage'.Fsys).load("InventoryPetsOriginSubDB").entries) do 
        PetAccessName =string.gsub(GetPetName(i) or i,"%p_-%a",string.upper)
        local Price = GetPrice2(i)
        if v.obtainable and not v.robux and Price and v.origin ~= "Desert Update" and v.origin ~= "Fall Update" and v.origin ~= "Rain" and v.origin ~= "Ocean Event" and v.origin ~= "Task Board Update" and not table.find(BuyPets, tostring(string.gsub(string.gsub(PetAccessName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. Price) and VerifyObtain(i) and i ~= "retired_egg" then 
            if v.origin_items and #v.origin_items == 0 then 
                if Settings.SelectedEgg == (tostring(string.gsub(string.gsub(PetAccessName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price .. " Bucks") then 
                    RS.API["ShopAPI/BuyItem"]:InvokeServer("pets", i, {})
                    return 
                end 
            end 
        end 
    end              
end})

Shop:AddToggle({Name="Auto Buy Eggs", Default=false, Callback=function(t)
    Settings.AutoBuyEggs = t 
    UpdateFile()
    while Settings.AutoBuyEggs and tonumber(require(game:service'ReplicatedStorage'.Fsys).load("ClientData").get("money")) >= 70 do task.wait()
        RS.API:FindFirstChild("ShopAPI/BuyItem"):InvokeServer("pets", Settings.SelectedEgg, {})
    end
end})


Shop:AddSection({
    Name = "// "..Cart.." Strollers //"
})

Shop:AddDropdown({Name="Select Stroller", Default=Settings.SelectedStroller,Options=Strollers, Callback=function(s)
    Settings.SelectedStroller = tostring(s)
end})

Shop:AddButton({Name="Buy Stroller", Callback=function()
    for i,v in pairs(StrollerTable.entries) do 
        local Price = tostring(v.cost)
        local StrollerName = string.gsub(i,"%p_-%a",string.upper)
        if v.cost ~= nil and not v.not_for_sale and (tostring(string.gsub(string.gsub(StrollerName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price) == Settings.SelectedStroller then 
            RS.API["ShopAPI/BuyItem"]:InvokeServer("strollers", tostring(i), {["chosen_rgb"]=Color3.new(1,1,1)})
            break 
        end
    end
    for i,v in pairs(game.ReplicatedStorage.SharedModules.ContentPacks:GetChildren()) do 
        if v:IsA("Folder") and v:FindFirstChild("InventorySubDB") then 
            local Folder = v:FindFirstChild("InventorySubDB")
            if Folder:FindFirstChild("Strollers") then
               local CheckFood = require(Folder.Strollers)
                for i,v in pairs(CheckFood) do 
                    local FoodName = v.name  
                    local Price = tostring(v.cost)
                    if v.cost ~= nil and not v.not_for_sale and not v.robux and not v.name:match("Potion") and v.source_content_pack ~= "Desert2022" and v.source_content_pack ~= "Snow2022" and v.source_content_pack ~= "Fall2022" and v.source_content_pack ~= "Winter2022" and Settings.SelectedStroller == tostring(string.gsub(string.gsub(FoodName, "_", " "), "^%l", function(c) return string.upper(c) end)) .. " | Price: " .. Price then 
                        RS.API["ShopAPI/BuyItem"]:InvokeServer("strollers", tostring(i), {["chosen_rgb"]=Color3.new(1,1,1)})
                        break 
                    end
                end
            end
        end
    end
end})

