        game.TextChatService.TextChannels.RBXGeneral:SendAsync("MaxFPS Enabled")
    end
    if not Settings.MaxPerformance and game.ReplicatedStorage:FindFirstChild('adoptmeisasscheeks014021') then 
        Library:MakeNotification({
            Name = "Eazvy Hub - Success",
            Content = "Disabled Max Performance; To make alot of client-functions, usable again please rejoin the game.",
