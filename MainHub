game:GetService("Players").LocalPlayer.PlayerGui.DailyLoginApp:GetPropertyChangedSignal("Enabled"):Connect(function()
    if game:GetService("Players").LocalPlayer.PlayerGui.DailyLoginApp.Enabled then
        for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.DailyLoginApp.Frame.Body.Buttons:GetChildren()) do 
            if v.Name == "ClaimButton" then
               pcall(function()
                    firesignal(v.MouseButton1Click)
                    if Settings.AutoRewards then 
                        GrabDailyReward()
                    end
                end)
            end
       end 
    end
end)


game:GetService("Players").LocalPlayer.PlayerGui.QuestIconApp.ImageButton.EventContainer.IsNew:GetPropertyChangedSignal("Position"):Connect(function()
    if NewTaskBool and Settings.AutoQuests then
        NewTaskBool = false
        Fsys("RouterClient").get("QuestAPI/MarkQuestsViewed"):FireServer()
        NewTask()
    end
end)

game:GetService("Players").LocalPlayer.PlayerGui.QuestIconApp.ImageButton.EventContainer.IsClaimable:GetPropertyChangedSignal("Position"):Connect(function()
    if NewClaimBool and Settings.AutoQuests then
        NewClaimBool = false
        NewClaim()
    end
end)

game:GetService("Players").LocalPlayer.PlayerGui.MinigameRewardsApp.Body:GetPropertyChangedSignal("Visible"):Connect(function()
    if Settings.EventFarm then 
        firesignal(game:GetService("Players").LocalPlayer.PlayerGui.MinigameRewardsApp.Body.Button.MouseButton1Click)
    end
end)


local AntiSpam = {}
local SpamDetected = false 

function isStringEnabled(text)
    local lowerText = text:lower()
    local isEnabled = lowerText:find("enabled") ~= nil
    return isEnabled, lowerText
end


xd = {}


function DetectSpam(text)
    local repetitionThreshold = 3
    local consecutiveCharacters = 0
    local repeatedCharacterCount = 0
    local uniqueCharacterCount = 0
    local previousChar = ""
    local repeatedSegments = {}
  
    for char in text:gmatch(".") do
        if char == previousChar then
            consecutiveCharacters = consecutiveCharacters + 1
            if consecutiveCharacters >= repetitionThreshold then
                repeatedCharacterCount = repeatedCharacterCount + 1
                local segment = text:sub(-consecutiveCharacters)
                repeatedSegments[segment] = (repeatedSegments[segment] or 0) + 1
            end
        else
            consecutiveCharacters = 0
            uniqueCharacterCount = uniqueCharacterCount + 1
        end
      
        previousChar = char
    end
  
    local textLength = #text
    local repetitionRatio = repeatedCharacterCount / textLength
    local uniquenessRatio = uniqueCharacterCount / textLength
  

    local repetitionThresholdRatio = 0.3
    local uniquenessThresholdRatio = 0.5
    local repeatedSegmentThreshold = 2
  
    if repetitionRatio >= repetitionThresholdRatio or uniquenessRatio <= uniquenessThresholdRatio then
        for segment, count in pairs(repeatedSegments) do
            if count >= repeatedSegmentThreshold then
                return true
            end
        end
    end
  
    return false
end


AntiSpam = {}
messageCount = {}
blockDuration = 180 
blockedUsers = {} 
timeThreshold = 5

game.TextChatService.TextChannels.RBXGeneral.MessageReceived:Connect(function(message)
    textSource = tostring(message.TextSource)
    text = tostring(message.Text)

    SpamDetected = false

    if game:GetService("Players"):FindFirstChild(textSource) and Settings.ChatLogger and textSource ~= tostring(game:GetService("Players").LocalPlayer.Name) then
        if blockedUsers[textSource] and blockedUsers[textSource] > os.time() then
            return
        end

        if not AntiSpam[textSource] then
            AntiSpam[textSource] = {}
            messageCount[textSource] = {}
        end

        currentTime = os.time()
        lastMessageTime = messageCount[textSource][text]

        if not lastMessageTime or (currentTime - lastMessageTime) > timeThreshold then
            messageCount[textSource][text] = currentTime
        else
            blockedUsers[textSource] = currentTime + blockDuration
            SpamDetected = true
        end

        if not SpamDetected then
            game.TextChatService.TextChannels.RBXGeneral:SendAsync(string.upper(textSource) .. " | @" .. tostring(game:GetService("Players")[textSource].DisplayName) .. " said: " .. text)
        elseif SpamDetected and tick() - (LastNotif or 0) > 6 then
            LastNotif = tick()
            game.TextChatService.TextChannels.RBXGeneral:SendAsync(string.upper(textSource) .. " | @" .. tostring(game:GetService("Players")[textSource].DisplayName) .. " has been silenced for attempting to spam chat system.")
        end
    end
end)


game:GetService("Players").LocalPlayer.PlayerGui.DialogApp:GetPropertyChangedSignal("Enabled"):Connect(function()
    task.wait(2)
    if game:GetService("Players").LocalPlayer.PlayerGui.DialogApp.Dialog.NormalDialog.Info.TextLabel.Text:match("Grabby") and Settings.EventFarm and not Settings.MaxPerformance then 
        Settings.StartingSoon = true 
        if Settings.EventFarm and Settings.Autofarm then 
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
            TeleportLocation("MainMap", "MainDoor", {})
            game.Workspace.Interiors:WaitForChild(tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model")))
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Teleports['Crab Minigame Join Zone'].ThirID + Vector3.new(0, -2, 0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
        end
        if Settings.EventFarm and not CheckMap() and not Settings.MaxPerformance then 
            Status:Set("Waiting for Minigame to Begin...")
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
            TeleportLocation("MainMap", "MainDoor", {})
            game.Workspace.Interiors:WaitForChild(tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model")))
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Teleports['Crab Minigame Join Zone'].ThirID + Vector3.new(0, -2, 0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
            repeat task.wait() 
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Teleports['Crab Minigame Join Zone'].ThirID + Vector3.new(0, -2, 0)
            until workspace.StaticMap.SummerJetskiMinigame["is_game_active"].Value == true or workspace.StaticMap.CrabMinigame_State.is_game_active.Value == true or not Settings.EventFarm
            Status:Set("Crab Minigame is starting...")
            Settings.StartingSoon = false 
        elseif CheckMap() and not Settings.MaxPerformance then 
            Status:Set("Waiting for Minigame to Begin...")
            repeat task.wait() 
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Teleports['Crab Minigame Join Zone'].ThirID + Vector3.new(0, -2, 0)
            until workspace.StaticMap.SummerJetskiMinigame["is_game_active"].Value == true or workspace.StaticMap.CrabMinigame_State.is_game_active.Value == true or not Settings.EventFarm
            Status:Set("Crab Minigame is starting...")
            Settings.StartingSoon = false 
        end 
    elseif game:GetService("Players").LocalPlayer.PlayerGui.DialogApp.Dialog.NormalDialog.Info.TextLabel.Text:match("Ice Cream") and Settings.EventFarm and not Settings.MaxPerformance then
        Status:Set("Waiting for Minigame to Begin...")
        if Settings.EventFarm and Settings.Autofarm then 
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
            TeleportLocation("MainMap", "MainDoor", {})
            game.Workspace.Interiors:WaitForChild(tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model")))
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Teleports['IceCream Minigame '].ThirID + Vector3.new(0, -2, 0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
        end
        repeat task.wait() until game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and workspace.StaticMap["IceCreamMinigame_State"]["is_game_active"].Value  
        Status:Set("Ice Cream Minigame is Starting...")
    elseif game:GetService("Players").LocalPlayer.PlayerGui.DialogApp.Dialog.NormalDialog.Info.TextLabel.Text:match("Scooting Stars") and Settings.EventFarm and not Settings.MaxPerformance then 
        Settings.StartingSoon = true 
        if Settings.EventFarm and Settings.Autofarm then 
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
            TeleportLocation("MainMap", "MainDoor", {})
            game.Workspace.Interiors:WaitForChild(tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model")))
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Teleports['Jetski Minigame '].ThirID + Vector3.new(0, -2, 0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
        end
        if Settings.EventFarm and not CheckMap() then 
            Status:Set("Waiting for Minigame to Begin...")
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
            TeleportLocation("MainMap", "MainDoor", {})
            game.Workspace.Interiors:WaitForChild(tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model")))
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Teleports['Jetski Minigame '].ThirID + Vector3.new(0, -2, 0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
            repeat task.wait() 
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Teleports['Jetski Minigame '].ThirID + Vector3.new(0, -2, 0)
            until workspace.StaticMap.SummerJetskiMinigame["is_game_active"].Value == true or workspace.StaticMap.CrabMinigame_State.is_game_active.Value == true or not Settings.EventFarm
            Status:Set("Jet-Ski Minigame is starting...")
            if CheckMap() and workspace.Interiors["MainMap/Default"].Summer.JetskiRaceMinigame:FindFirstChild("RampVFX") then 
                workspace.Interiors["MainMap/Default"].Summer.JetskiRaceMinigame:FindFirstChild("RampVFX"):Destroy()
            end
            if CheckMap() and workspace.Interiors["MainMap/Default"].Summer.JetskiRaceMinigame:FindFirstChild('RampCurves') then 
                workspace.Interiors["MainMap/Default"].Summer.JetskiRaceMinigame:FindFirstChild('RampCurves'):Destroy()
            end
            if CheckMap() and workspace.Interiors["MainMap/Default"].Summer:FindFirstChild("Visual") then 
                for i,v in pairs(workspace.Interiors["MainMap/Default"].Summer.Visual:GetChildren()) do 
                    if v.Name ~= "SandTerrain" then
                       v:Destroy()
                    end 
                end 
            end
            Settings.StartingSoon = false 
        elseif CheckMap() then 
            local Previous_Destination = Fsys("ClientData").get_server(game:GetService("Players").LocalPlayer, "char_wrapper").location
            Status:Set("Waiting for Minigame to Begin...")
            -- RS.API:FindFirstChild("LocationAPI/SubscribeToInteriorFurniture"):FireServer(Previous_Destination.full_destination_id)
            -- RS.API:FindFirstChild("LocationAPI/SetLocation"):FireServer(Previous_Destination.destination_id)
            repeat task.wait() 
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Teleports['Jetski Minigame '].ThirID + Vector3.new(0, -2, 0)
            until workspace.StaticMap.SummerJetskiMinigame["is_game_active"].Value == true or workspace.StaticMap.CrabMinigame_State.is_game_active.Value == true or not Settings.EventFarm
            Status:Set("Jet-Ski Minigame is starting...")
            if CheckMap() and workspace.Interiors["MainMap/Default"].Summer.JetskiRaceMinigame:FindFirstChild("RampVFX") then 
                workspace.Interiors["MainMap/Default"].Summer.JetskiRaceMinigame:FindFirstChild("RampVFX"):Destroy()
            end
            if CheckMap() and workspace.Interiors["MainMap/Default"].Summer.JetskiRaceMinigame:FindFirstChild('RampCurves') then 
                workspace.Interiors["MainMap/Default"].Summer.JetskiRaceMinigame:FindFirstChild('RampCurves'):Destroy()
            end
            if CheckMap() and workspace.Interiors["MainMap/Default"].Summer:FindFirstChild("Visual") then 
                for i,v in pairs(workspace.Interiors["MainMap/Default"].Summer.Visual:GetChildren()) do 
                    if v.Name ~= "SandTerrain" then
                       v:Destroy()
                    end 
                end 
            end
            Settings.StartingSoon = false 
        end
    end 
    if game:GetService("Players").LocalPlayer.PlayerGui.DialogApp.Enabled and Settings.EventFarm and not Settings.MaxPerformance then 
        if game.Players.LocalPlayer.PlayerGui.DialogApp.Dialog.NormalDialog.Info.TextLabel.Text:match("starting soon") then 
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
            TeleportLocation("Halloween2023", "MainDoor",{})
            game.Workspace.Interiors:WaitForChild(tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model")))
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Interiors[tostring(game.Workspace.Interiors:FindFirstChildWhichIsA("Model"))].Doors.MainDoor.WorkingParts.TouchToEnter.CFrame:ToWorldSpace(CFrame.new(0, 0, -35))
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
            task.wait(.15)
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Teleports['TileSkip Minigame Join Zone'].ThirID + Vector3.new(0, -2, 0)
            foundMini = "" 
            for _,v in pairs(workspace.StaticMap:GetChildren()) do 
                if v:IsA("Folder") and v:FindFirstChild("is_game_active") then 
                    foundMini = v 
                    break 
                end
            end 
            repeat task.wait() until foundMini ~= "" and foundMini.is_game_active.Value 
            if game:GetService("Workspace").Interiors:FindFirstChild("TileSkipMinigame") then 
                for i,v in pairs(game:GetService("Workspace").Interiors.TileSkipMinigame.Minigame.Tiles:GetChildren()) do 
                    if v:IsA("Model") and v:FindFirstChild("Collider") then 
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart,v.Collider,0)
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart,v.Collider,1)
                    end 
                end 
            end
        end 
        if game:GetService("Players").LocalPlayer.PlayerGui.DialogApp.Dialog.NormalDialog.Info.TextLabel.Text == "Gingerbread has appeared all over the lake! Teleport there now?" then 
            for _,v in pairs(game:GetService("ReplicatedStorage").Resources.IceSkating.GingerbreadMarkers:GetChildren()) do 
                game:GetService("ReplicatedStorage").API["WinterEventAPI/PickUpGingerbread"]:InvokeServer(tostring(v))
            end
            game:GetService("ReplicatedStorage").API["WinterEventAPI/RedeemPendingGingerbread"]:FireServer()
        elseif game:GetService("Players").LocalPlayer.PlayerGui.DialogApp.Dialog.NormalDialog.Info.TextLabel.Text == "Snowball Fight is starting soon! Teleport there now?" and Settings.EventFarm then 
        end
    end
end)

